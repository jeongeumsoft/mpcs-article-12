{
    "name": "mpcs/article",
    "description": "mpcs core : article extention",
    "type": "library",
    "authors": [
        {
            "name": "HWAJUN LEE",
            "email": "ezrock@naver.com"
        }
    ],
    "require": {
        "cviebrock/eloquent-taggable": "^12.0",
        "mpcs/bootstrap5": "~0.3.0|dev-master",
        "mpcs/core": "~0.3.0|dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mpcs\\Article\\": "src/"
        }
    },
